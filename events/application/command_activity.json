{
  "caption": "Command Activity",
  "description": "Command Activity events report the state and status of commands.",
  "extends": "application",
  "name": "command_activity",
  "uid": 5,
  "attributes": {
    "command_name": {
      "group": "primary",
      "requirement": "required"
    },
    "command_uid": {
      "group": "primary",
      "requirement": "recommended"
    },
    "activity_id": {
      "enum": {
        "1": {
          "caption": "Submitted"
        },
        "2": {
          "caption": "Queued"
        },
        "3": {
          "caption": "Started"
        },
        "4": {
          "caption": "Cancel Requested"
        },
        "5": {
          "caption": "Completed"
        },
        "6": {
          "caption": "Canceled"
        },
        "7": {
          "caption": "Error"
        },
        "8": {
          "caption": "Rejected"
        }
      }
    }
  }
}